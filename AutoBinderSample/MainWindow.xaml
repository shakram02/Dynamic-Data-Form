<Window x:Class="DynamicDataFormSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DynamicDataFormSample"
        xmlns:autoBinder="clr-namespace:DynamicDataForm;assembly=AutoBinder"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    
    <StackPanel>
        <!-- Custom empty user control -->
        <autoBinder:AutoBinder></autoBinder:AutoBinder>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="75" Click="button_Click"/>
        
    </StackPanel>
</Window>
